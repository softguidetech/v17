<?xml version="1.0"?>
<odoo>


	<record id="thiqah_res_stakeholder_view_form"
		model="ir.ui.view">
		<field name="name">res.stakeholder.form</field>
		<field name="model">res.stakeholder</field>
		<field name="arch" type="xml">
			<form class="o_res_stakeholder_form" js_class="crm_form">

				<sheet>
					<div class="oe_title">
						<label for="name" />
						<h1>
							<field class="o_text_overflow" name="name" />
						</h1>
					</div>
					
					
				</sheet>
			</form>
		</field>
	</record>


	<record id="thiqah_res_stakeholder_view_tree"
		model="ir.ui.view">
		<field name="name">res.stakeholder.tree</field>
		<field name="model">res.stakeholder</field>
		<field name="arch" type="xml">
			<tree>
				<field name="name" />
			</tree>
		</field>
	</record>

	<record id="thiqah_res_stakeholder_action"
		model="ir.actions.act_window">
		<field name="name">stakeholders</field>
		<field name="res_model">res.stakeholder</field>
		<field name="view_mode">tree,form</field>
	</record>




</odoo>
