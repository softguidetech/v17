<?xml version="1.0" encoding="UTF-8"?>



<odoo>

	<record id="res_config_settineegs_view_form" model="ir.ui.view">
		<field name="name">res.config.settings.vieeew.form</field>
		<field name="model">res.config.settings</field>
<!--		<field name="inherit_id" ref="base.res_config_settings_view_form"/>-->
		<field name="inherit_id" ref="base_setup.res_config_settings_view_form"/>
		<field name="arch" type="xml">
<!--			<xpath expr="//div[hasclass('settings')]" position="inside">-->
				<xpath expr="//form" position="inside">
				<div class="col-12 col-lg-6 o_setting_box">
					<div class="o_setting_left_pane"></div>
					<div class="o_setting_right_pane">
						<label for="binary_max_size"/>
						<div class="text-muted">
			                Maximum allowed file size in MB
						</div>
						<div class="mt8">
							<field name="binary_max_size" class="o_light_label"/>
						</div>
					</div>
				</div>
				<div class="col-12 col-lg-6 o_setting_box">
					<div class="o_setting_right_pane"></div>
					<div class="o_setting_left_pane">
						<label for="supported_types"/>
						<div class="text-muted">
			                Supported Types
						</div>
						<div class="mt8">
							<field name="supported_types" class="o_light_label"/>
						</div>
					</div>
				</div>
			</xpath>
		</field>
	</record>

</odoo>