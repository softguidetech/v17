<?xml version="1.0" encoding="utf-8"?>
<data>

    <record id="thiqah_project_res_config_settings_form" model="ir.ui.view">
        <field name="name">res.config.settings.view.form.inherit.project.thiqah
        </field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="project.res_config_settings_view_form"/>
        <field name="arch" type="xml">
<!--            <xpath expr="//div[hasclass('app_settings_block')][@data-key='project']" position="inside">-->
            <xpath expr="//form" position="inside">
                <h2>Assignments</h2>
                <div class="col-12 col-lg-12 o_setting_box">
                    <group colspan="4">
                        <group>
                            <field name="assignment_advisor_id" />
                            <field name="digital_solution__id" />
                        </group>
                    </group>
                </div>
            </xpath>
        </field>
    </record>



</data>
