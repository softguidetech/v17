<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="risk_type_tree" model="ir.ui.view">
        <field name="name">risk_type.tree</field>
        <field name="model">thiqah.project.risk.type</field>
        <field name="arch" type="xml">
            <tree editable="bottom">
                <field name="code"/>
                <field name="name_ar"/>
                <field name="name_en"/>
            </tree>
        </field>
    </record>


    <record id="risk_type_form" model="ir.ui.view">
        <field name="name">thiqah.project.risk.type.form</field>
        <field name="model">thiqah.project.risk.type</field>
        <field name="arch" type="xml">
            <form string="">
                <header>
                </header>
                <sheet>
                    <group name="name">
                        <field name="code"/>
                        <field name="name_ar"/>
                        <field name="name_en"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>


</odoo>
